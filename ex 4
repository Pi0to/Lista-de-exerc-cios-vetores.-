
#include <stdio.h>
#define t 200

int main() {
    
    int i, p=1, tam;
    char s[t];
    
    printf("Digite uma ou mais frase(s) com no maximo 200 caracteres: ");
    fgets(s, t, stdin);
    fflush(stdin);
    
    tam = strlen(s);
    
    for(i=0;i<tam;i++) if(s[i] == ' ') p++;
    
    printf("A(s) frase(s) possuem %d palavras.", p);
    
    return 0;
}
