// Online C compiler to run C program online
#include <stdio.h>
#define t 3

int main() {
    
    int v[t], i, p, j;
    
    p = 0;
    
    for(i=0;i<t;i++){
        printf("Digite o %d° valor do vetor: ", i+1);
        scanf("%d", &v[i]);
    }
    
    
    for(i=0;i<t;i++)
        
        if (v[i]==2) if (v[i]>p) p=v[i];
        
        
        else
            
            for(j=2;j<v[i];j++)
                
                if ((v[i] % j) != 0) 
                    
                    if (v[i]>p) p=v[i];
        
    
    
    if (p==0) printf("Nenhum número é primo");
    else printf("O maior numero primo eh %d.", p);
    

    return 0;
}
